{
  "name": "app2",
  "version": "1.0.0",
  "description": "My PWA App",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "app": "cross-env PORT=3001 nuxt dev -c app/nuxt.config.js",
    "app:build": "nuxt build -c app/nuxt.config.js",
    "app:start": "nuxt start -c app/nuxt.config.js",
    "api": "cross-env PORT=3000 SUPPRESS_NO_CONFIG_WARNING=1 nodemon node_modules/bak/bin/bak start api",
    "api:start": "bak start api"
  },
  "dependencies": {
    "@bakjs/auth": "^3.0.3",
    "@bakjs/mongo": "^2.0.4",
    "@nuxtjs/axios": "^4.5.2",
    "@nuxtjs/proxy": "^1.1.3",
    "@nuxtjs/pwa": "^2.0.5",
    "bak": "^3.0.5",
    "cross-env": "^5.1.3",
    "crossenv": "^0.0.2-security",
    "framework7": "^2.0.6",
    "framework7-icons": "^0.8.9",
    "material-icons": "^0.1.0",
    "node-sass": "^4.7.2",
    "nuxt": "^1.1.1",
    "nuxt7": "latest",
    "router": "^1.3.2",
    "sass-loader": "^6.0.6"
  }
}
